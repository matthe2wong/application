<!doctype html>
<html lang="zh">
<head>
<meta charset="utf-8">
<style type="text/css">
table td, table th {
	border: 1px solid #000;
}
</style>
</head>
<body>
	<table>
		<thead>
			<tr>
					<th>是否开启   1是 2否 is_open</th>
					<th>标题 title</th>
					<th>开始时间 start_time</th>
					<th>结束时间 end_time</th>
					<th>日期 date</th>

			</tr>
		</thead>
		<tbody>
            <?php if ($this->data): foreach($this->data as $d):?>
            <tr>
					<td style="mso-number-format='\@'"><?php echo $d->is_open ?></td>
					<td style="mso-number-format='\@'"><?php echo $d->title ?></td>
					<td style="mso-number-format='yyyy-mm-dd h:i:s'"><?php echo $d->start_time ? date("Y-m-d H:i:s", strtotime($d->start_time)) : "" ?></td>
					<td style="mso-number-format='yyyy-mm-dd h:i:s'"><?php echo $d->end_time ? date("Y-m-d H:i:s", strtotime($d->end_time)) : "" ?></td>
					<td style="mso-number-format='yyyy-mm-dd h:i:s'"><?php echo $d->date ? date("Y-m-d H:i:s", strtotime($d->date)) : "" ?></td>
			</tr>
            <?php endforeach; endif; ?>
        </tbody>
	</table>
</body>
</html>
